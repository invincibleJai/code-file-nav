{
  "name": "code-file-nav",
  "displayName": "File Navigator",
  "description": "A file system navigator that allows for actions to be performed in the file system using only the keyboard via the quick pick palette",
  "version": "0.1.1",
  "publisher": "jakelucas",
  "engines": {
    "vscode": "^0.10.10"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.codeFileNav"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.codeFileNav",
        "title": "Code File Nav"
      }
    ],
    "keybindings": [
      {
        "command": "extension.codeFileNav",
        "key": "ctrl+l",
        "mac": "ctrl+l"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "drivelist": "^2.0.11"
  }
}
